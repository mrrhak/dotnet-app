      "applicationUrl": "https://localhost:7282;http://localhost:5083"
    },
    "Dotnet App (PROD)": {
      "commandName": "Project",
      "launchBrowser": true,
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Production"
      },
      "dotnetRunMessages": true,
      "applicationUrl": "https://localhost:7282;http://localhost:5083"
